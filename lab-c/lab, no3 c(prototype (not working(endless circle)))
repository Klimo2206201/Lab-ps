        scanf("%f",&x2);
        printf("Введите шаг(step): ");
        scanf("%d",&step);
        h = ((x2-x1)/(step));
        while(x1<x2)
        {
        x=x1;
        x1+=h;    
            if ((Y > 1)||(Y < -1))
            {
                Y=acos(-7*a*a - 10*a*x + 8*x*x + 1);
                printf  ("Y=%f\n\n", Y);
