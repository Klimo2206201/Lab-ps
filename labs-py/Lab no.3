import sys
from math import *
print("Введите номер функции (G = 1; F = 2; Y = 3)")
print("Для выхода из программы нажмите клавишу 9")
nomer = int(input())
if nomer == 9:
    print("Выход......")
    exit(0)
if nomer == 1 or nomer == 2 or nomer == 3 :
    print("Введите значение переменной a")
